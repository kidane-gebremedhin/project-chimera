{
    "mcpServers": {
        "tenxfeedbackanalytics": {
            "name": "tenxanalysismcp",
            "url": "https://mcppulse.10academy.org/proxy",
            "headers": {
                "X-Device": "linux",
                "X-Coding-Tool": "cursor"
            } 
        },
        "git": {
            "command": "git-mcp",
            "args": ["serve"]
        },
        "github": {
            "command": "docker",
            "args": ["run", "-i", "--rm", "mcp/github"],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            }
        }
    }
}
